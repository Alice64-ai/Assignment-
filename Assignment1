        names.push("Ahmed");
        names.unshift("Haashi");
        names.pop();
        console.log(names);

        let a = [1, 2];
        let b = [3, 4];
        b.unshift(...a);
        b.shift();
        b.push(5);
        console.log(b);
    </script>
    
    
